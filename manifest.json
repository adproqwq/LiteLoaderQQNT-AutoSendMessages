  "version": "1.5.2",
  "icon": "./assets/icon.svg",
  "thumb": "./assets/icon.svg",
  "authors": [
    {
      "name": "Adpro",
      "link": "https://github.com/adproqwq"
    }
  ],
  "platform": [
    "win32",
    "linux",
    "darwin"
  ],
  "dependencies": [
    "euphony",
    "LiteLoaderQQNT_CheckUpdateModule"
  ],
  "injects": {
    "main": "./main/index.js",
    "preload": "./preload/index.js",
    "renderer": "./renderer/index.js"
